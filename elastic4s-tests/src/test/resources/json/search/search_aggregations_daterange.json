{
    "aggs": {
        "daterange_agg": {
            "date_range": {
                "field": "date",
                "missing": "wibble",
                "ranges": [
                    {
                        "from": "now-1Y",
                        "to": "now"
                    },
                    {
                        "from": "now-3m||"
                    }
                ]
            }
        }
    }
}
